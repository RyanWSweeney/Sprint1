
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()
GET     /login                      controllers.HomeController.loginHandler()

GET     /signup                     controllers.HomeController.signup()

GET     /register                   controllers.HomeController.signupHandler()

GET     /forgotPassword             controllers.HomeController.forgotPassword()

GET   /application                  controllers.HomeController.application()

GET     /edit                       controllers.HomeController.edit()

GET     /edit2                controllers.HomeController.edit2()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
